@model AlertMessage
<div class="row">
    <div class="col-md-12">
        <div class="alert alert-@Model.AlertType alert-dismissable">
            <button type="button" class="close" data-dismiss="alert" aria-hidden="true"></button>
            <strong>@Model.Title !</strong> @Model.Message
        </div>
    </div>
</div>


